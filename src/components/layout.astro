        theme.onResize();
      });

      jQuery(window).load(function () {
        if (location.hash !== "") {
          var hash = "#" + window.location.hash.substr(1);
          if (hash.length) {
            jQuery("html,body")
              .delay(0)
              .animate(
                {
                  scrollTop: jQuery(hash).offset().top - 44 + "px",
                },
                {
                  duration: 1200,
                  easing: "easeInOutExpo",
                }
              );
          }
        }
      });
    </script>
